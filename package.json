{
  "name": "andelabootcamp",
  "version": "1.0.0",
  "description": "Node RESTful API",
  "main": "app.js",
  "scripts": {
    "start": "nodemon server/bin/dev",
    "test": "nyc mocha --require babel-register server/test/*.js --timeout 10000 --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "clean": "rm -rf dist",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "build": "npm run clean && mkdir dist && babel server -s -d dist",
    "production": "npm run build && node bin/production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tolumide-ng/AndelaBootcamp.git"
  },
  "author": "tolumide",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tolumide-ng/AndelaBootcamp/issues"
  },
  "homepage": "https://github.com/tolumide-ng/AndelaBootcamp#readme",
  "devDependencies": {
    "chai-http": "^4.2.1",
    "coveralls": "^3.0.2",
    "eslint": "^5.12.0",
    "istanbul": "^0.4.5",
    "mocha-lcov-reporter": "^1.3.0"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "cors": "^2.8.5",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.3",
    "express": "^4.16.4",
    "mocha": "^5.2.0",
    "moment": "^2.23.0",
    "nodemon": "^1.18.9",
    "nyc": "^13.1.0",
    "supertest": "^3.3.0",
    "uuid": "^3.3.2"
  }
}
